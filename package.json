{
  "name": "tactical-alert",
  "version": "1.1.1",
  "description": "A validation library for Flux applications",
  "license": "MIT",
  "main": "src/index.js",
  "keywords": [
    "flux",
    "store",
    "react",
    "reactjs",
    "validator",
    "validations"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "examples": "watchify ./examples/app.js -o ./dist/_bundle.js & npm run local_server",
    "local_server": "echo Open examples on http:localhost:8000/examples && python -m SimpleHTTPServer",
    "lint": "eslint ./src",
    "test": "jest"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "ext": ".js"
        }
      ]
    ]
  },
  "jest": {
    "scriptPreprocessor": "./util/jest_preprocessor.js",
    "unmockedModulePathPatterns": [
      "./node_modules/react"
    ],
    "testFileExtensions": [
      "js"
    ]
  },
  "peerDependencies": {
    "lcars": "1.1.x",
    "cargo-bay": "1.1.x"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "cargo-bay": "^1.1.0",
    "lcars": "^1.1.0",
    "eslint": "^0.14.1",
    "jest-cli": "^0.2.2",
    "reactify": "^1.0.0",
    "watchify": "^2.3.0",
    "react": "^0.13.0",
    "react-tools": "^0.13.0"
  },
  "dependencies": {
    "amp-merge": "^1.0.0",
    "memory-alpha": "^1.0.0"
  },
  "contributors": [
    "rtorr <rtorruellas@gmail.com> (http://rtorr.com/)"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:sstate/tactical-alert.git"
  }
}
