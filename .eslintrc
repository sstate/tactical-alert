{
  "env": {
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx": true,
    "nomen": true
  },
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "eol-last": 0,
    "space-infix-ops": 0,
    "new-cap": 0
  }
}